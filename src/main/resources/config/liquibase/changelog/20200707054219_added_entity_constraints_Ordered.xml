<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Ordered.
    -->
    <changeSet id="20200707054219-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="pay_id"
                                 baseTableName="ordered"
                                 constraintName="fk_ordered_pay_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pay"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="ordered"
                                 constraintName="fk_ordered_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="ordered"
                                 constraintName="fk_ordered_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item"/>

    </changeSet>
</databaseChangeLog>
